[run]
source = .
omit =
    */migrations/*
    */tests/*
    */__pycache__/*
    */admin.py
    */apps.py
    manage.py
    config/wsgi.py
    config/asgi.py

[report]
exclude_lines =
    pragma: no cover
    def __str__
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    if settings.DEBUG:
    except Exception: